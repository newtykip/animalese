{
  "svelte.enable-ts-plugin": true,
  "files.exclude": {
    "node_modules": true,
    ".svelte-kit": true,
    "build": true,
    "src-tauri/gen": true,
    "src-tauri/target": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
